# -*- mode: ruby -*-
# vi: set ft=ruby :

# Simple Vagrantfile enabling you to be able to play with this repo without installing things on your machine

Vagrant.configure("2") do |config|
    config.vm.provider "virtualbox" do |vb|
      vb.memory = "4096"
      vb.cpus = 4
      vb.vm.box = "gusztavvargadr/windows-server"
      vb.vm.box_version = "1809.0.2006.standard-core"
      vb.vm.synced_folder ".", "c:\\vagrant"
    end
  end
